<div class="centered">
  <h1>Your Projects</h1>
  <div class="pure-menu">
    <ul class="pure-menu-list" hx-boost="true" hx-target="body">
      <% projects.forEach((project) => { %>
        <li class="pure-menu-item">
          <a class="pure-menu-link" href="/projects/<%- project.projectId %>">
            <%= project.name %>
          </a>
        </li>
      <% }); %>
    </ul>
  </div>
</div>
